{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.{css,js,json,png,jpg,jpeg,gif,svg,ico,woff,woff2,ttf,eot}"
    ]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "globalHeaders": {
    "cache-control": "no-cache"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "text/javascript",
    ".css": "text/css"
  },
  "platform": {
    "apiRuntime": "node:20"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html"
    }
  },
  "environmentVariables": {
    "NEXT_PUBLIC_API_URL": "http://40.83.89.157:8000"
  }
}
